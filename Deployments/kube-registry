apiVersion: apps/v1
kind: Deployment
metadata:
 name: kube-registry
 labels:
  type: registry
spec:
 template:
  metadata:
   name: kube-registry
   labels:
    type: registry
  spec:
   containers:
   - image: registry:2
     name: kube-registry
     ports:
      ContainerPort: 5000
 selector:
  matchLabels:
   type: registry
 replicas: 2
